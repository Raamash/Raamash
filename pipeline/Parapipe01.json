					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "inputspdataset",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"pipe_container": {
				"type": "string",
				"defaultValue": "ramaccount"
			},
			"pipe_directory_inp": {
				"type": "string",
				"defaultValue": "datacontain1"
			},
			"pipe_directory_out": {
				"type": "string",
				"defaultValue": "dataopcontain2"
			},
			"pipe_file_name": {
				"type": "string",
				"defaultValue": "SalesLTProduct"
			}
		},
		"annotations": []
	}
}